<Keyboard xmlns:android="http://schemas.android.com/apk/res-auto"
    android:keyWidth="9%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height_5_rows"
    >

    <Row>

        <Key android:codes="43" android:keyLabel="+"
            android:horizontalGap="5%"
            android:keyEdgeFlags="left"
            />
        <Key android:codes="45" android:keyLabel="-"/>
        <Key android:codes="215" android:keyLabel="×"/>
        <Key android:codes="247" android:keyLabel="÷"/>
        <Key android:codes="42" android:keyLabel="*"/>
        <Key android:codes="9055" android:keyLabel="⍟"/>
        <Key android:codes="9017" android:keyLabel="⌹"/>
        <Key android:codes="9675" android:keyLabel="○"/>
        <Key android:codes="33" android:keyLabel="!"/>
        <Key android:codes="63" android:keyLabel="\?"
            android:keyEdgeFlags="right"/>

    </Row>

    <Row>
        <Key android:codes="8592" android:keyLabel="←"
            android:keyEdgeFlags="left"
            />
        <Key android:codes="8900" android:keyLabel="⋄"
            android:horizontalGap="5%"
            />
        <Key android:codes="9053" android:keyLabel="⍝"/>
        <Key android:codes="8594" android:keyLabel="→"/>
        <Key android:codes="9077" android:keyLabel="⍵"/>
        <Key android:codes="9082" android:keyLabel="⍺"/>
        <Key android:codes="8711" android:keyLabel="∇"/>
        <Key android:codes="38" android:keyLabel="&amp;"/>

        <Key android:codes="8744" android:keyLabel="∨"
            android:horizontalGap="5%"
            />
        <Key android:codes="8743" android:keyLabel="∧"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="61" android:keyLabel="="
            android:keyEdgeFlags="left"/>
        <Key android:codes="8800" android:keyLabel="≠"/>
        <Key android:codes="8804" android:keyLabel="≤"/>
        <Key android:codes="60" android:keyLabel="&lt;"/>
        <Key android:codes="62" android:keyLabel="&gt;"/>
        <Key android:codes="8805" android:keyLabel="≥"/>
        <Key android:codes="8801" android:keyLabel="≡"/>
        <Key android:codes="8802" android:keyLabel="≢"/>

        <Key android:codes="9073" android:keyLabel="⍱"
            android:horizontalGap="10%"
            />
        <Key android:codes="9074" android:keyLabel="⍲"
            android:keyEdgeFlags="right"/>

    </Row>

    <Row>

        <Key android:codes="8593" android:keyLabel="↑"
            android:keyEdgeFlags="left"/>
        <Key android:codes="8595" android:keyLabel="↓"/>
        <Key android:codes="8834" android:keyLabel="⊂"/>
        <Key android:codes="8835" android:keyLabel="⊃"/>
        <Key android:codes="9015" android:keyLabel="⌷"/>
        <Key android:codes="9035" android:keyLabel="⍋"/>
        <Key android:codes="9042" android:keyLabel="⍒"/>

        <Key android:codes="8710" android:keyLabel="∆"
            android:horizontalGap="5%"
            />
        <Key android:codes="175" android:keyLabel="¯"
            android:horizontalGap="5%"
            />
        <Key android:codes="9068" android:keyLabel="⍬"
        android:keyEdgeFlags="right"/>
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-1" android:keyLabel="1/2"
            android:keyWidth="15%p"
            android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyLabel="123" android:keyWidth="15%p"/>
        <Key android:codes="-6" android:keyLabel="ABC" android:keyWidth="15%p"/>
        <Key android:codes="32"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:keyWidth="25%p"
            android:isRepeatable="true"
            />
        <Key android:codes="-5"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            android:keyWidth="15%p"
            android:isRepeatable="true"
            />
        <Key android:codes="10"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyWidth="15%p"
            android:keyEdgeFlags="right"
            />
    </Row>
</Keyboard>